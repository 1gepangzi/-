V0
#include<stdio.h>
#include<string.h>
int main()
{
	char ch[]="3+4";
	int a=ch[0]-'0';
	int b=ch[2]-'0';
	int c=a+b;
	printf("%d",c);
	return 0;
}

V1
#include<stdio.h>
#include<string.h>
int main()
{
	char ch[]="1+2+2+1+2+5+4-1-3+4-8";
	int c=ch[0]-'0';
	for(int i=0;i<strlen(ch);i++)
	{
		if(ch[i]=='+')
		{
			int a=ch[i+1]-'0';
			c=c+a;
			i++;
			//TODO
		}
		else if(ch[i]=='-')
		{
			int b=ch[i+1]-'0';
			c=c+b;
			i++;
			//TODO
		}
		//TODO
	}
	printf("%d",c);
	return 0;
}

V2
#include<stdio.h>
#include<string.h>
int main()
{
	char ch[]="2*2/4*1/1*2*3/2";
	int x=ch[0]-'0';
	for(int i=1;i<=strlen(ch);i++){
		if(ch[i]=='*'){
			int a=ch[i+1]-'0';
			x=x*a;
			i++;
			//TODO
		}
		else if(ch[i]=='/'){
			int b=ch[i+1]-'0';
			x=x/b;
			i++;
			//TODO
		}
		//TODO
	}
	printf_s("x=%d",x);
	return 0;
}

V3
#include<stdio.h>
#include<string.h>
int main()
{
	char ch[]="1+2*3+2/2-1";
	char strTmp[strlen(ch)];
	int strtmpindex=-1;
	for(int i=0;i<=strlen(ch);i++){
		if(ch[i]=='*')
		{
			int leftoperand=strTmp[strtmpindex]-'0';
			int rightoperand=strTmp[strtmpindex]-'0';
			strTmp[strtmpindex]=leftoperand*rightoperand+'0';
			i++;
		}
		else if(ch[i]=='/'){
			int leftoperand=strTmp[strtmpindex]-'0';
			int rightoperand=strTmp[strtmpindex]-'0';
			strTmp[strtmpindex]=leftoperand/rightoperand+'0';
			i++;
			//TODO
		}
		else strTmp[++strtmpindex]=ch[i];
		//TODO
	}
	int res=strTmp[0]-'0';
	for(int i=1;i<=strlen(strTmp);i++)
	{
		if(strTmp[i]=='+')
		res=res+strTmp[++i]-'0';
		else if(strTmp[i]=='-')
		res=res-strTmp[++i]-'0';
		}
		//TODO
		printf_s("%d\n",res);
	return 0;
}
